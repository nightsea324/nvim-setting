{
    "json.enable": true,
    "json.format.enable": true,
    "explorer.presets": {
        ".vim": {
            "root-uri": "~/.vim"
        },
        "floating": {
            "position": "floating",
            "floating-width": 100
        }
    },
    "explorer.file.hiddenRules": {
        "extensions": ["o", "a", "obj", "pyc"],
        "filenames": [".git", ".DS_Store", ".vscode"],
        "patternMatches": []
    },
    "explorer.width": 30,
    "explorer.file.showHiddenFiles": true,
    "explorer.icon.enableNerdfont": true,
    "explorer.keyMappings.global": {
        "i": false,
        "k": "nodePrev",
        "j": "nodeNext",
        "*": "toggleSelection",
        "<tab>": "actionMenu",
        "l": ["expandable?", "expand", "open"],
        "J": ["toggleSelection", "nodeNext"],
        "K": ["toggleSelection", "nodePrev"],
        "gl": "expandRecursive",
        "gh": "collapseRecursive",
        "o": ["expanded?", "collapse", "expand"],
        "<cr>": ["expandable?", "cd", "open"],
        "e": "open",
        "s": "open:split",
        "v": "open:vsplit",
        "t": "open:tab",
        "<bs>": "gotoParent",
        "gp": "preview:labeling",
        "y": "copyFilepath",
        "Y": "copyFilename",
        "c": "copyFile",
        "x": "cutFile",
        "p": "pasteFile",
        "d": "delete",
        "D": "deleteForever",
        "a": "addFile",
        "A": "addDirectory",
        "r": "rename",
        ".": "toggleHidden",
        "R": "refresh",
        "?": "help",
        "q": "quit",
        "<esc>": "esc",
        "X": "systemExecute",
        "f": "search",
        "F": "searchRecursive",
        "gf": "gotoSource:file",
        "gb": "gotoSource:buffer",
        "[[": "sourcePrev",
        "]]": "sourceNext",
        "[d": "diagnosticPrev",
        "]d": "diagnosticNext",
        "[g": "gitPrev",
        "]g": "gitNext",
        "<<": "gitStage",
        ">>": "gitUnstage"
    },
    "coc.preferences.formatOnSaveFiletypes": [
        "css",
        "yaml",
        "javascript",
        "typescript",
        "json",
        "go",
        "html",
        "proto"
    ],
    "prettier.tabWidth": 4,
    "prettier.singleQuote": false,
    "prettier.arrowParens": "always",
    "tsserver.enable": true,
    "snippets.extends": {
        "javascript": ["javascript"],
        "typescript": ["javascript"]
    },
    "git.addGBlameToVirtualText": true,
    "git.blameUseRealTime": true,
    "git.enableGutters": true
}
